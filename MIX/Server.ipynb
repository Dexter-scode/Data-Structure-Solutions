    "import socket\n",
    "s = socket.socket()\n",
    "\n",
    "s.bind(('localhost',59589))\n",
    "\n",
    "s.listen(3)\n",
    "\n",
    "while True:\n",
    "    c,address = s.accept()\n",
    "    name = c.recv(1024).decode()\n",
    "    print(address,name)\n",
    "    c.send(bytes(\"Hello\",'utf-8'))\n",
    "    c.close()\n",
    "    "
